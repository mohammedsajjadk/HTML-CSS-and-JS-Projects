{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Sajjad's Boilerplate project for HTML, CSS and JS projects",
  "main": "index.js",
  "scripts": {
    "watch:sass-2": "node-sass src/sass/style.scss src/css/style.css -w",
    "watch:sass": "node-sass src/sass/style.scss dist/css/style.css -w",
    "dev-server": "webpack-dev-server --mode development --open",
    "start": "npm-run-all --parallel dev-server watch:sass",
    "build": "webpack --mode production",
    "go": "npm-run-all --parallel watch:sass nodemon build dev-server",
    "production": "npm-run-all build compile:sass concat:css prefix:css compress:css",
    "compile:sass": "node-sass src/sass/style.scss dist/css/style.comp.css",
    "concat:css": "concat -o dist/css/style.concat.css dist/css/icon-font.css dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/css/style.concat.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed"
  },
  "author": "Sajjad",
  "license": "MS",
  "devDependencies": {
    "autoprefixer": "<10.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concat": "^1.0.3",
    "html-webpack-plugin": "^4.5.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "mysql": "^2.18.1",
    "pug": "^3.0.0",
    "tether": "^1.4.7",
    "uniqid": "^5.2.0",
    "uuid": "^8.3.1"
  }
}
